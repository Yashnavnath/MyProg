<property name="city" value="Mumbai"/>
<property name="salary" value="55000"/>
<property name="department" ref="d2" />
</bean>

<bean id="bean3" class="com.springcore.Q12.Employee">
<property name="empId" value="103"/>
<property name="name" value="Niraj Sharma"/>
<property name="city" value="Pune"/>
<property name="salary" value="30000"/>
<property name="department" ref="d1" />
</bean>
<bean id="bean4" class="com.springcore.Q12.Employee">
<property name="empId" value="104"/>
